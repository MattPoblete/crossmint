# Crossmint API Configuration
# Server-side API key (used by API proxy, not exposed to browser)
CROSSMINT_API_KEY=sk_staging_your_server_api_key
# Client-side API key (used for SDK initialization, can be public)
NEXT_PUBLIC_CROSSMINT_API_KEY=client_api_key
NEXT_PUBLIC_CROSSMINT_PROJECT_ID=project_id

# Network Configuration
# Options: testnet | mainnet
STELLAR_NETWORK=testnet

# Stellar RPC URLs (optional, defaults will be used if not set)
# STELLAR_RPC_URL_TESTNET=https://soroban-testnet.stellar.org
# STELLAR_RPC_URL_MAINNET=https://soroban.stellar.org

# Soroswap API (required for DEX operations)
NEXT_PUBLIC_SOROSWAP_API_KEY=your_soroswap_api_key

# Defindex API (optional)
# DEFINDEX_API_KEY=your_defindex_api_key

