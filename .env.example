# Crossmint API Configuration
NEXT_PUBLIC_CROSSMINT_API_KEY=your_api_key_here
NEXT_PUBLIC_CROSSMINT_PROJECT_ID=your_project_id_here

# Crossmint Staging API Key (for integration tests)
CROSSMINT_STAGING_API_KEY=your_staging_api_key_here

# Network Configuration
